    <PackageReference Include="MudBlazor" Version="6.1.9" />
    <PackageReference Include="Syncfusion.Blazor.RichTextEditor" Version="20.4.0.50" />
    <PackageReference Include="Syncfusion.Blazor.Themes" Version="20.4.0.50" />
    <PackageReference Include="Syncfusion.Blazor.Grid" Version="20.4.0.50" />
