  <ItemGroup>
    <None Remove="cssStyles\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="cssStyles\" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\SharedServices\SharedServices.csproj">
    </ProjectReference>
  </ItemGroup>
