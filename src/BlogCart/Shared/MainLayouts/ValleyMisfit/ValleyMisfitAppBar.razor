@inherits LayoutComponentBase
@namespace BlogCart.Shared.MainLayouts
@inject NavigationManager NavigationManager




<MudAppBar Elevation="21" Dense="true">
    <MudIconButton Icon="@Icons.Material.Filled.Menu" Color="Color.Inherit" Size="Size.Medium" Edge="Edge.End"
                   OnClick="async () => await OnSidebarToggled.InvokeAsync()" aria-label="Toggle Menu sidebar" />
    <MudIconButton Href="/valleymisfit" Icon="@Icons.Material.Filled.Home" Color="Color.Inherit" Size="Size.Medium" aria-label="Go to Home Page" />
    <MudSpacer />
    <MudIconButton Href="#" Icon="@Icons.Material.Filled.Email" Color="Color.Inherit" Size="Size.Medium" Edge="Edge.Start" aria-label="Go to Email" />
    <MudIconButton Href="https://www.facebook.com" Icon="@Icons.Custom.Brands.Facebook" Color="Color.Inherit" Size="Size.Medium" Edge="Edge.Start" aria-label="Go to Facebook" />
    <MudIconButton Href="https://www.instagram.com" Icon="@Icons.Custom.Brands.Instagram" Color="Color.Inherit" Size="Size.Medium" Edge="Edge.Start" aria-label="Go to Instagram" />
    <MudIconButton Href="https://www.twitter.com" Icon="@Icons.Custom.Brands.Twitter" Color="Color.Inherit" Size="Size.Medium" Edge="Edge.Start" aria-label="Go to Twitter" />
</MudAppBar>

@code {
    [Parameter]
    public EventCallback OnSidebarToggled { get; set; }
}





